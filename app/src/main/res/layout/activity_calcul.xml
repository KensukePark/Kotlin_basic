<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".CalculActivity">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/appbar_container"
        android:name="com.example.myproject2.AppbarFragment"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="#EADDFF"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/app_title2"
        android:textSize="20dp"
        android:fontFamily="sans-serif"
        app:layout_constraintEnd_toEndOf="@id/appbar_container"
        app:layout_constraintStart_toStartOf="@id/appbar_container"
        app:layout_constraintTop_toTopOf="@id/appbar_container"
        app:layout_constraintBottom_toBottomOf="@id/appbar_container"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/string_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="30dp"
        android:textColor="@color/grey_600"
        app:layout_constraintTop_toBottomOf="@id/appbar_container"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_margin="15dp"
        />
    <TextView
        android:id="@+id/string_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="50dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@id/button_devide"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_margin="15dp"
        />
    <Button
        android:id="@+id/button_00"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="00"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button_0"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="0"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintLeft_toRightOf="@id/button_00"
        app:layout_constraintWidth_percent="0.25" />
    <Button
        android:id="@+id/button_spot"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="."
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintLeft_toRightOf="@id/button_0"
        app:layout_constraintWidth_percent="0.25" />
    <Button
        android:id="@+id/button_equal"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="="
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintLeft_toRightOf="@id/button_spot"
        app:layout_constraintWidth_percent="0.25" />

    <Button
        android:id="@+id/button_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="1"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/button_00" />

    <Button
        android:id="@+id/button_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="2"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintLeft_toRightOf="@id/button_1"
        app:layout_constraintBottom_toTopOf="@id/button_0" />
    <Button
        android:id="@+id/button_3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="3"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintLeft_toRightOf="@id/button_2"
        app:layout_constraintBottom_toTopOf="@id/button_spot"/>
    <Button
        android:id="@+id/button_plus"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="+"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintLeft_toRightOf="@id/button_3"
        app:layout_constraintBottom_toTopOf="@id/button_equal"/>

    <Button
        android:id="@+id/button_4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="4"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/button_1" />

    <Button
        android:id="@+id/button_5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="5"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintLeft_toRightOf="@id/button_4"
        app:layout_constraintBottom_toTopOf="@id/button_2" />
    <Button
        android:id="@+id/button_6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="6"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintLeft_toRightOf="@id/button_5"
        app:layout_constraintBottom_toTopOf="@id/button_3"/>
    <Button
        android:id="@+id/button_minus"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="-"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintLeft_toRightOf="@id/button_6"
        app:layout_constraintBottom_toTopOf="@id/button_plus"/>

    <Button
        android:id="@+id/button_7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="7"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/button_4" />

    <Button
        android:id="@+id/button_8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="8"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintLeft_toRightOf="@id/button_7"
        app:layout_constraintBottom_toTopOf="@id/button_5" />
    <Button
        android:id="@+id/button_9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="9"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintLeft_toRightOf="@id/button_8"
        app:layout_constraintBottom_toTopOf="@id/button_6"/>
    <Button
        android:id="@+id/button_multi"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="×"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintLeft_toRightOf="@id/button_9"
        app:layout_constraintBottom_toTopOf="@id/button_minus"/>

    <Button
        android:id="@+id/button_CE"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="CE"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/button_7" />

    <Button
        android:id="@+id/button_C"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="C"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintLeft_toRightOf="@id/button_CE"
        app:layout_constraintBottom_toTopOf="@id/button_8" />
    <Button
        android:id="@+id/button_del"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="←"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintLeft_toRightOf="@id/button_C"
        app:layout_constraintBottom_toTopOf="@id/button_9"/>
    <Button
        android:id="@+id/button_devide"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:background="@drawable/rect_button"
        android:padding="0dp"
        android:text="÷"
        android:textSize="24dp"
        android:textColor="@color/black"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintLeft_toRightOf="@id/button_del"
        app:layout_constraintBottom_toTopOf="@id/button_multi"/>

</androidx.constraintlayout.widget.ConstraintLayout>